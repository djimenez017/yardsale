<div class="field column is-half is-offset-one-half mx-auto">
  <label class="label has-text-centered"
    >Search Yard sales by City or Zip Code</label
  >
  <div class="control is-flex is-flex-direction-row">
    <input class="input" type="text" placeholder="Los Angeles" />
    <button class="button is-primary is-fullwidth">Search</button>
  </div>
</div>

<% if @yardsales.any? %> <% @yardsales.each do |yardsale| %>
<div class="card column is-three-quarters-tablet mx-auto my-5">
  <a href="<%= yardsale_path(yardsale) %>" class="has-text-black">
    <!-- <%= link_to "Yardsale", yardsale_path(yardsale) %> -->
    <div class="columns">
      <div class="column is-4">
        <div class="card-image">
          <figure class="image is-4by3">
            <img
              src="data:image/jpeg;base64,"
              alt="Placeholder image"
            />
          </figure>
        </div>
      </div>
      <div class="column is-8">
        <div class="card-content p-0">
          <div class="media">
            <div class="media-content">
              <p class="title is-4 m-0"><%= yardsale.title %></p>
              <time class="p-0" datetime="2016-1-1">
                <%= yardsale.date.to_date.strftime("%A, %B %d, %Y") %></time
              >
            </div>
          </div>
          <div class="content">
            <%= truncate(yardsale.description, length:200) %>
            <br />
            <br />
            <div>
              <span class="tag is-primary">Collectibles</span>
              <span class="tag is-primary">Clothing</span>
              <span class="tag is-primary">Electronics</span>
            </div>
          </div>
        </div>
      </div>
    </div>
  </a>
</div>
<% end %> <% else %>
<div class="column is-three-quarters-tablet mx-auto">
  <p>No Yardsales available at this time 😧</p>
</div>
<% end %>
